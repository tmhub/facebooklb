            ["app/design/frontend/default/default/template/facebooklb","app/design/frontend/default/default/template/facebooklb"],
            ["app/etc/modules/TM_FaceBookLB.xml","app/etc/modules/TM_FaceBookLB.xml"],
            ["app/locale/en_US/TM_FacebookLB.csv","app/locale/en_US/TM_FacebookLB.csv"]
