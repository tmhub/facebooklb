{
    "name": "tm/facebooklb",
    "type": "magento-module",
    "homepage": "https://github.com/tmhub/facebooklb",
    "license": "OSL-3.0",
    "description": "Facebook Like Button integration",
    "version": "2.0.0",
    "authors": [
        {
            "name": "Templates-Master Team",
            "email": "support@templates-master.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "tm/core": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://tmhub.github.io/packages/"
